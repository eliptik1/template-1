{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-use-before-define": [
            "error",
            {
                "functions": false,
                "classes": true,
                "variables": true,
                "allowNamedExports": false
            }
        ],
        "func-names": "off",
        "no-unused-vars": "off",
        "indent": [
            2,
            "tab"
        ],
        "no-tabs": 0
    }
}